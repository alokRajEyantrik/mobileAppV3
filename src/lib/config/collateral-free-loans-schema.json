{
	"formId": "loanForm_v1",
	"title": "Loan Application",
	"pages": [
		{
			"id": "firstPage",
			"title": "Let's Rock!",
			"nextButtonVisibility": {
				"mode": ["allRequiredAnswered"]
			},
			"questions": [
				{
					"id": "q1_loanName",
					"bindsTo_template": "loanName",
					"contextKey": "loanName",
					"type": "radio",
					"uiGroup": "loan_details",
					"required": true,
					"question": "What are you looking for?",
					"description": "Get started by letting us know a little bit about what you need",
					"options": [
						{
							"label": "Home Loan",
							"value": "Home Loan",
							"flagKey": {
								"isSecuredLoan": true,
								"isHomeLoan": true
							}
						},
						{
							"label": "Plot Loan",
							"value": "Plot Loan",
							"flagKey": {
								"isSecuredLoan": true,
								"isPlotLoan": true
							}
						},
						{
							"label": "Loan Against Property (LAP)",
							"value": "Loan Against Property",
							"flagKey": {
								"isSecuredLoan": true,
								"isLAP": true
							}
						},
						{
							"label": "Business Loan",
							"value": "Business Loan",
							"flagKey": {
								"isUnsecuredLoan": true,
								"isBusinessLoan": true
							}
						},
						{
							"label": "Professional Loan",
							"value": "Professional Loan",
							"flagKey": {
								"isUnsecuredLoan": true,
								"isProfessionalLoan": true
							}
						},
						{
							"label": "Personal Loan",
							"value": "Personal Loan",
							"flagKey": {
								"isUnsecuredLoan": true,
								"isPersonalLoan": true
							}
						}
					]
				},
				{
					"id": "q2_unSecureLoanType",
					"bindsTo_template": "unSecureLoanType",
					"contextKey": "unSecureLoanType",
					"type": "radio",
					"uiGroup": "loan_details",
					"required": true,
					"question": "Select the form in which you want to avail this unsecured loan.",
					"description": "This helps us identify the most suitable loan structure for you. For details, check our Drop-line Overdraft (DOD) Page.",
					"options": [
						{
							"label": {
								"var": "q1_loanName"
							},
							"value": {
								"var": "q1_loanName"
							}
						},
						{
							"label": "Drop-line Overdraft (DOD)",
							"value": "Drop-line OverDraft (DOD)"
						}
					],
					"showWhen": {
						"in": [
							{
								"var": "loanName"
							},
							["Personal Loan", "Professional Loan", "Business Loan"]
						]
					}
				},
				{
					"id": "q3_existingLoan",
					"bindsTo_template": "existingLoan",
					"contextKey": "existingLoan",
					"type": "radio",
					"uiGroup": "loan_details",
					"required": true,
					"question": "Do you have any existing obligations?",
					"description": "This includes all types of loans and credit limits you may have, such as personal loans, home loans, car loans, credit card loans (excluding monthly dues), gold loans, consumer durable loans, DODs, CCs, ODs, and any other ongoing liabilities.",
					"options": [
						{
							"label": "Yes",
							"value": "Yes",
							"flagKey": {
								"isObligation": true
							}
						},
						{
							"label": "No",
							"value": "No",
							"flagKey": {
								"isObligation": false
							}
						}
					],
					"showWhen": {
						"in": [
							{
								"var": "unSecureLoanType"
							},
							["Personal Loan", "Professional Loan", "Business Loan", "Drop-line OverDraft (DOD)"]
						]
					}
				},
				{
					"id": "q4_loanType",
					"bindsTo_template": "loanType",
					"contextKey": "loanType",
					"type": "radio",
					"uiGroup": "loan_details",
					"required": true,
					"question": "How you'd like to proceed with your loan?",
					"description": "Tell us if you’re applying for a fresh loan or want to consolidate your current loans—fully or partially—with or without extra borrowing. If you only want debt consolidation, you can enter zero as the additional funds, later.",
					"options": [
						{
							"label": "Start Fresh with New Loan",
							"value": "Start Fresh with New Loan"
						},
						{
							"label": "Debt Consolidation with Extra Funds",
							"value": "Debt Consolidation with Extra Funds"
						}
					],
					"showWhen": {
						"and": [
							{
								"in": [
									{
										"var": "unSecureLoanType"
									},
									["Personal Loan", "Professional Loan", "Business Loan"]
								]
							},
							{
								"==": [
									{
										"var": "existingLoan"
									},
									"Yes"
								]
							}
						]
					}
				}
			]
		},
		{
			"id": "collateral_free_selectionPage",
			"title": "A few check points !",
			"nextButtonVisibility": { "mode": ["allRequiredAnswered"] },
			"questions": [
				{
					"id": "q1_ITRAvailable",
					"bindsTo_template": "ITRAvailable",
					"contextKey": "ITRAvailable",
					"type": "radio",
					"uiGroup": "loan_details",
					"required": true,
					"question": "Have you filed Income Tax Returns (ITRs) for the last 3 financial years?",
					"options": [
						{
							"label": "Yes",
							"value": "Yes",
							"flagKey": {
								"isITRAvailable": true
							}
						},
						{
							"label": "No",
							"value": "No",
							"flagKey": {
								"isITRAvailable": false
							}
						}
					],
					"showWhen": {
						"in": [
							{
								"var": "unSecureLoanType"
							},
							["Business Loan", "Professional Loan"]
						]
					},
					"errorMessage": {
						"required": true,
						"message": "We regret to inform you that we are unable to process your request at this time due to the following reasons: ITR for last 3 years not available."
					},
					"validation": {
						"condition": {
							"!=": [
								{
									"var": "ITRAvailable"
								},
								"Yes"
							]
						}
					}
				},
				{
					"id": "q2_haveCurrentAccount",
					"bindsTo_template": "haveCurrentAccount",
					"contextKey": "haveCurrentAccount",
					"type": "radio",
					"uiGroup": "loan_details",
					"required": true,
					"question": "Do you have Current, OD, or CC accounts in any bank with a total active history of 3 years or more?",
					"options": [
						{
							"label": "Yes",
							"value": "Yes",
							"flagKey": {
								"haveCurrentAccount": true
							}
						},
						{
							"label": "No",
							"value": "No",
							"flagKey": {
								"haveCurrentAccount": false
							}
						}
					],
					"showWhen": {
						"and": [
							{
								"!=": [
									{
										"var": "ITRAvailable"
									},
									""
								]
							},
							{
								"==": [
									{
										"var": "ITRAvailable"
									},
									"Yes"
								]
							}
						]
					},
					"errorMessage": {
						"required": true,
						"message": "We regret to inform you that we are unable to process your request at this time due to the following reasons: You do not have a current Account, Overdraft (OD), or Cash Credit (CC)."
					},
					"validation": {
						"condition": {
							"!=": [
								{
									"var": "haveCurrentAccount"
								},
								"Yes"
							]
						}
					}
				},
				{
					"id": "q3_gstRegistration",
					"bindsTo_template": "gstRegistration",
					"contextKey": "gstRegistration",
					"type": "radio",
					"uiGroup": "loan_details",
					"required": true,
					"question": "Is the GST registration with the current account three years old?",
					"options": [
						{
							"label": "Yes",
							"value": "Yes",
							"flagKey": {
								"isGstRegistration": true
							}
						},
						{
							"label": "No",
							"value": "No",
							"flagKey": {
								"isGstRegistration": false
							}
						}
					],
					"showWhen": {
						"and": [
							{
								"!=": [
									{
										"var": "haveCurrentAccount"
									},
									""
								]
							},
							{
								"==": [
									{
										"var": "haveCurrentAccount"
									},
									"Yes"
								]
							}
						]
					},
					"errorMessage": {
						"required": true,
						"message": "We regret to inform you that we are unable to process you request at this time due to the following reasons: GST registration with current account not three year old."
					},
					"validation": {
						"condition": {
							"!=": [
								{
									"var": "gstRegistration"
								},
								"Yes"
							]
						}
					}
				},
				{
					"id": "q4_GSTNumber",
					"bindsTo_template": "GSTNumber",
					"contextKey": "GSTNumber",
					"type": "text",
					"uiGroup": "text_fields",
					"uiMeta": {
						"placeholder": "Enter your GST Number"
					},
					"required": true,
					"question": "Please enter your GST Number?",
					"showWhen": {
						"and": [
							{
								"!=": [
									{
										"var": "gstRegistration"
									},
									""
								]
							},
							{
								"==": [
									{
										"var": "gstRegistration"
									},
									"Yes"
								]
							}
						]
					},
					"errorMessage": {
						"required": "GST Number is required",
						"lengthError": "GST Number must be exactly 15 characters long.",
						"message": "We regret to inform you that we are unable to process your request at this time due to the following reasons: GST registration with current account not three years old.",
						"stateNotServed": "We're not serve their"
					},
					"validation": {
						"condition": {
							"or": [
								{
									"==": [
										{
											"var": "GSTNumber"
										},
										""
									]
								},
								{
									"!=": [
										{
											"var": "GSTNumber.length"
										},
										15
									]
								}
							]
						},
						"message": "validateGSTState"
					}
				},
				{
					"id": "q5_isApplicantIsNRI",
					"bindsTo_template": "ApplicantIsNRI",
					"contextKey": "ApplicantIsNRI",
					"type": "radio",
					"uiGroup": "loan_details",
					"required": true,
					"question": "If applicant or any of the co-applicants is NRI?",
					"options": [
						{
							"label": "Yes",
							"value": "Yes",
							"flagKey": { "isApplicantIsNRI": true }
						},
						{
							"label": "No",
							"value": "No",
							"flagKey": { "isApplicantIsNRI": false }
						}
					],
					"showWhen": {
						"and": [
							{ "!=": [{ "var": "GSTNumber" }, ""] },
							{ "or": [{ "var": "loanName" }, ["Professional Loan", "Business Loan"]] }
						]
					},
					"errorMessage": {
						"required": true,
						"message": "We regret to inform you that we are unable to process your request at this time due to the following reasons: Applicant is an NRI."
					},
					"validation": {
						"condition": {
							"==": [{ "var": "ApplicantIsNRI" }, "Yes"]
						},
						"message": "validateisNRI"
					}
				},
				{
					"id": "q_isPaySlipAvailable",
					"bindsTo_template": "payslips",
					"contextKey": "payslips",
					"type": "radio",
					"uiGroup": "loan_details",
					"required": true,
					"question": "If payslips are available for last 6 months ?",
					"options": [
						{
							"label": "Yes",
							"value": "Yes",
							"flagKey": {
								"isPaySlipAvailable": true
							}
						},
						{
							"label": "No",
							"value": "No",
							"flagKey": {
								"isPaySlipAvailable": false
							}
						}
					],
					"showWhen": {
						"in": [
							{
								"var": "unSecureLoanType"
							},
							["Personal Loan"]
						]
					},
					"errorMessage": {
						"required": true,
						"message": "We regret to inform you that we are unable to process you request at this time due to the following reasons: Payslips not available."
					},
					"validation": {
						"condition": {
							"!=": [
								{
									"var": "payslips"
								},
								"Yes"
							]
						}
					}
				},
				{
					"id": "q_isForm16Available",
					"bindsTo_template": "Form16Available",
					"contextKey": "Form16Available",
					"type": "radio",
					"uiGroup": "loan_details",
					"required": true,
					"question": "If latest Form-16 is available ?",
					"options": [
						{
							"label": "Yes",
							"value": "Yes",
							"flagKey": {
								"isForm16Available": true
							}
						},
						{
							"label": "No",
							"value": "No",
							"flagKey": {
								"isForm16Available": false
							}
						}
					],
					"showWhen": {
						"in": [
							{
								"var": "unSecureLoanType"
							},
							["Personal Loan"]
						]
					},
					"errorMessage": {
						"required": true,
						"message": "We regret to inform you that we are unable to process you request at this time due to the following reasons: From16 not available."
					},
					"validation": {
						"condition": {
							"!=": [
								{
									"var": "Form16Available"
								},
								"Yes"
							]
						}
					}
				},
				{
					"id": "q_isApplicantIsNRI",
					"bindsTo_template": "ApplicantIsNRI",
					"contextKey": "ApplicantIsNRI",
					"type": "radio",
					"uiGroup": "loan_details",
					"required": true,
					"question": "If applicant or any of the co-applicants is NRI?",
					"options": [
						{
							"label": "Yes",
							"value": "Yes",
							"flagKey": {
								"isApplicantIsNRI": true
							}
						},
						{
							"label": "No",
							"value": "No",
							"flagKey": {
								"isApplicantIsNRI": false
							}
						}
					],
					"showWhen": {
						"in": [{ "var": "loanName" }, ["Personal Loan"]]
					},
					"errorMessage": {
						"required": true,
						"message": "We regret to inform you that we are unable to process you request at this time due to the following reasons: Applicant is NRI."
					},
					"validation": {
						"condition": {
							"!=": [
								{
									"var": "ApplicantIsNRI"
								},
								"Yes"
							]
						}
					}
				}
			]
		},
		{
			"id": "locationPage",
			"title": "Business Location",
			"nextButtonVisibility": {
				"mode": ["allRequiredAnswered"]
			},
			"questions": [
				{
